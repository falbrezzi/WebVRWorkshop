   "outputs": [],
   "source": [
    "cone <- function(x, y){\n",
    "sqrt(x^2+y^2)\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "x <- y <- seq(-1, 1, length= 20)\n",
    "z <- outer(x, y, cone)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "persp(x, y, z)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "persp(x, y, z,\n",
    "main=\"Perspective Plot of a Cone\",\n",
    "zlab = \"Height\",\n",
    "theta = 30, phi = 15,\n",
    "col = \"springgreen\", shade = 0.5)"
   ]
  },
  {
   "cell_type": "code",
